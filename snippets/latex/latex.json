{
  "ThreePartTable": {
    "prefix": "threetable",
    "body": [
      "\\begin{ThreePartTable}",
      "\t\\footnotesize",
      "\t\\setstretch{1}",
      "\t\\begin{TableNotes}[flushleft]",
      "\t\t\\normalsize",
      "\t\t\\centering",
      "\t\t\\item \\elaboradopor",
      "\t\\end{TableNotes}",
      "\t\t\\begin{longtable}{l l}",
      "\t\t\t\\caption{$1}",
      "\t\t\t\\label{table:$2} \\\\\\\\",
      "\t\t\t\\toprule",
      "\t\t\t\\multicolumn{1}{c}{\\textbf{$3}} & ",
      "\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
      "\t\t\t\\midrule",
      "\t\t\t\\endfirsthead",
      "\t\t\t\\toprule",
      "\t\t\t\\multicolumn{1}{c}{\\textbf{$3}} & ",
      "\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
      "\t\t\t\\midrule",
      "\t\t\t\\endhead",
      "\t\t\ta & b \\\\\\\\",
      "\t\t\tc & d \\\\\\\\",
      "\t\t\t$5",
      "\t\t\t\\bottomrule",
      "\t\t\\end{longtable}",
      "\t\t%\\insertTableNotes",
      "\\end{ThreePartTable}",
      "$0"
    ],
    "description": "Add a three part long table"
  },
  "historia": {
    "prefix": "historia",
    "body": [
      "\\historia{historiacaption = Historia de usuario - $1,",
      "\thistorialabel = {table:historia-$2},",
      "\tusuario = ${3},",
      "\tnombre =  $1,",
      "\tprioridad = ${4:Alta},",
      "\triesgo = ${5:Alta},",
      "\tpuntos = ${6:1},",
      "\titeracion = ${7:1},",
      "\tdescripcion = {$8},",
      "\tobservaciones = {$9},",
      "}"
    ],
    "description": "Historia de usuario"
  },

  "prettyref": {
    "prefix": "pref",
    "body": "\\prettyref{$1}",
    "description": "Pretty Reference"
  },
  "listing": {
    "prefix": "lst",
    "body": [
      "\\begin{hyphen}",
      "\t\\begin{lstlisting}[caption={$1}, label={lst:$2}]",
      "\t\t$3",
      "\t\\end{lstlisting}",
      "\\end{hyphen}"
    ],
    "description": "Listado"
  }
}
